{
  "name": "cdek-widget-solidjs",
  "version": "0.0.0",
  "description": "A small library that wraps CDEK-widget for solidjs users",
  "license": "MIT",
  "author": "danila-schelkov",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danila-schelkov/cdek-widget-solidjs.git"
  },
  "homepage": "https://github.com/danila-schelkov/cdek-widget-solidjs#readme",
  "bugs": {
    "url": "https://github.com/danila-schelkov/cdek-widget-solidjs/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --max-warnings 0 \"src/**/*.{js,ts,tsx,jsx}\"",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.6",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^9.31.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "jsdom": "^26.1.0",
    "prettier": "3.6.2",
    "solid-js": "^1.9.7",
    "tsup": "^8.5.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-plugin-solid": "^2.11.7",
    "vitest": "^3.2.4",
    "@eslint/compat": "^1.3.1",
    "@yandex/ymaps3-types": "^1.0.16966980",
    "yup": "^1.6.1"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0.0"
  }
}
